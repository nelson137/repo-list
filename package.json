{
    "name": "repo-list-website",
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "lint": "prettier --check --plugin-search-dir=. . && eslint .",
        "format": "prettier --write --plugin-search-dir=. .",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "build": "vite build",
        "dev": "vite dev",
        "test": "vitest",
        "coverage": "vitest run --coverage",
        "preview": "svelte-kit sync && vite preview",
        "prepare": "svelte-kit sync",
        "package": "svelte-kit package"
    },
    "devDependencies": {
        "@anatine/zod-mock": "^3.13.2",
        "@sveltejs/adapter-node": "^1.3.1",
        "@sveltejs/kit": "^1.30.3",
        "@testing-library/jest-dom": "^6.0.1",
        "@testing-library/svelte": "^4.0.3",
        "@types/cookie": "^0.5.1",
        "@types/lodash-es": "^4.17.6",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "@vitest/coverage-v8": "^0.34.1",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.3.0",
        "jsdom": "^22.1.0",
        "prettier": "^2.6.2",
        "prettier-plugin-svelte": "^2.10.1",
        "sass": "^1.66.0",
        "svelte": "^4.2.16",
        "svelte-check": "^3.4.3",
        "svelte-preprocess": "^5.0.3",
        "tslib": "^2.3.1",
        "typescript": "^5.0.0",
        "vite": "^4.3.9",
        "vitest": "^0.34.1"
    },
    "dependencies": {
        "@github/details-menu-element": "^1.0.12",
        "@octokit/auth-oauth-app": "^4.3.2",
        "@svelteuidev/composables": "^0.15.3",
        "@svelteuidev/core": "^0.15.3",
        "cookie": "^0.5.0",
        "lodash-es": "^4.17.21",
        "octokit": "^1.8.1",
        "svelecte": "^3.16.4",
        "svelte-simple-modal": "^1.6.1",
        "uuid": "^8.3.2",
        "zod": "^3.21.4"
    },
    "overrides": {
        "svelte-simple-modal": {
            "svelte": "$svelte"
        }
    }
}
